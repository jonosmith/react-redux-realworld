{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-var-requires": false,
    "member-access": false,
    "interface-name": false,
    "ordered-imports": [
      true,
      {
        "named-imports-order": "lowercase-last"
      }
    ],
    "object-literal-sort-keys": false
  }
}
